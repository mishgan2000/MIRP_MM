#include <stdint.h>;
#include <stdio.h>;
#include "xparameters.h";

#define ddd *((volatile uint32_t *)(XPAR_MIKE_IP_0_BASEADDR))
int main(void){
uint32_t temp = 0;
uint8_t i;
	while(1){

		temp = ddd;
		if(temp == 0)
			for(i = 0; i < 10; i++) {}
		else if(temp == 1)
			for(i = 0; i < 20; i++) {}
		else if(temp == 2)
			for(i = 0; i < 30; i++) {}
		else if(temp == 3)
			for(i = 0; i < 40; i++) {}
		else if(temp == 4)
			for(i = 0; i < 40; i++) {}

	}
}
